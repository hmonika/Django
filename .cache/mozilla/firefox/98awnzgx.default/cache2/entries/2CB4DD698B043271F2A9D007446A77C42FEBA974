function mailto(button, param) {
	var ctf = this;
	var t = ctf.getCurrentProduct();

// Suite Solutions
// 4 Nov 2010
// Add the word Topic: to subject
// 13 Nov 2012
// mailto address should come from plugin_customization.ini
	window.location = "mailto:" + param + "?subject=Topic: " + ctf.getCurrentTopic() + " " + t; 						
} 
